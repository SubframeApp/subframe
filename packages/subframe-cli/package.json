{
  "name": "@subframe/cli",
  "version": "1.178.0",
  "description": "Subframe's CLI tool for syncing your code with Subframe designs.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "bin": {
    "main-sync": "./bin/main-sync.js"
  },
  "files": [
    "dist/",
    "bin/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "build-for-publish": "npm run ts && npm run build",
    "dev": "rollup -c --watch",
    "update-version": "npm version minor",
    "minor-publish": "npm run build-for-publish && npm run publish-to-npm",
    "publish-to-npm": "npm publish --access public",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "ts": "tsc --noEmit",
    "ts:watch": "tsc --watch --incremental --noEmit"
  },
  "keywords": [
    "subframe"
  ],
  "author": "Irvin Zhan",
  "license": "ISC",
  "dependencies": {
    "@antfu/ni": "0.21.8",
    "@segment/analytics-node": "2.1.2",
    "chalk": "4.1.2",
    "commander": "^13.1.0",
    "comment-json": "4.2.3",
    "degit": "^2.8.4",
    "detect-indent": "7.0.1",
    "execa": "7.2.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-webcrypto": "^2.3.8",
    "ora": "8.2.0",
    "prompts": "2.4.2",
    "semver": "7.6.3",
    "shared": "*",
    "ts-morph": "19.0.0",
    "xdg-app-paths": "8.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.23.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "15.2.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/degit": "^2.8.6",
    "@types/prompts": "2.4.5",
    "@types/segment-analytics": "0.0.34",
    "jest": "29.7.0",
    "rollup": "3.29.5",
    "rollup-plugin-esbuild": "^6.2.1",
    "typescript": "^5.1.3"
  }
}

{
  "name": "@subframe/cli",
  "version": "1.198.0",
  "description": "Subframe's CLI tool for syncing your code with Subframe designs.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build-for-publish": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c --bundleConfigAsCjs --watch",
    "update-version": "npm version minor",
    "minor-publish": "npm run build-for-publish && npm run publish-to-npm",
    "publish-to-npm": "npm publish --access public",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "ts": "tsc --noEmit",
    "ts:watch": "tsc --watch --incremental --noEmit"
  },
  "keywords": [
    "subframe"
  ],
  "author": "Irvin Zhan",
  "license": "ISC",
  "bin": {
    "main-sync": "./bin/main-sync.js"
  },
  "dependencies": {
    "@antfu/ni": "0.21.8",
    "@bluwy/giget-core": "^0.1.6",
    "@commander-js/extra-typings": "^13.1.0",
    "@segment/analytics-node": "2.1.2",
    "chalk": "4.1.2",
    "commander": "^13.1.0",
    "comment-json": "4.2.3",
    "detect-indent": "7.0.1",
    "execa": "7.2.0",
    "node-fetch": "2.7.0",
    "ora": "8.2.0",
    "prompts": "2.4.2",
    "proxy-agent": "^6.5.0",
    "semver": "7.6.3",
    "ts-morph": "19.0.0",
    "xdg-app-paths": "8.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.23.0",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "15.2.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "0.4.4",
    "@types/node-fetch": "^2.6.13",
    "@types/prompts": "2.4.5",
    "@types/segment-analytics": "0.0.34",
    "rollup": "3.29.5",
    "rollup-plugin-typescript2": "0.34.1",
    "typescript": "^5.1.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/"
  ]
}

{
  "name": "@subframe/cli",
  "version": "1.167.0",
  "description": "Subframe's CLI tool for syncing your code with Subframe designs.",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "NODE_ENV=production tsc --noEmit && rollup -c rollup.config.js && npm run shebang",
    "dev": "NODE_ENV=development rollup -c rollup.config.js --watch",
    "start": "node bin/init-main.js",
    "build-for-publish": "npm run build",
    "update-version": "npm version minor",
    "shebang": "sed -i '' '1s/^/#!\\/usr\\/bin\\/env node\\n/' bin/init-main.js",
    "minor-publish": "npm run build-for-publish && npm run publish-to-npm",
    "publish-to-npm": "npm publish --access public",
    "ts": "tsc --noEmit",
    "ts:watch": "tsc --watch --incremental --noEmit"
  },
  "keywords": [
    "subframe"
  ],
  "author": "Irvin Zhan",
  "license": "ISC",
  "bin": {
    "init-main": "./bin/init-main.js"
  },
  "dependencies": {
    "@antfu/ni": "0.21.8",
    "@commander-js/extra-typings": "^13.1.0",
    "@segment/analytics-node": "2.1.2",
    "chalk": "4.1.2",
    "commander": "^13.1.0",
    "comment-json": "4.2.3",
    "degit": "^2.8.4",
    "detect-indent": "7.0.1",
    "execa": "7.2.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-webcrypto": "^2.3.8",
    "node-fetch": "2.7.0",
    "ora": "8.2.0",
    "prompts": "2.4.2",
    "semver": "7.6.3",
    "ts-morph": "19.0.0",
    "xdg-app-paths": "8.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.2.2",
    "@rollup/plugin-replace": "^6.0.2",
    "shared": "*",
    "@types/degit": "^2.8.6",
    "@types/prompts": "2.4.5",
    "@types/segment-analytics": "0.0.34",
    "esbuild": "^0.25.0",
    "jest": "29.7.0",
    "rollup": "3.29.5",
    "rollup-plugin-esbuild": "^6.2.0",
    "typescript": "5.1.3"
  },
  "files": [
    "bin/",
    "dist/",
    "package.json"
  ]
}
